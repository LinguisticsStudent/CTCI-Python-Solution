string1 = "abcdefg"
string2 = "abcdefa"

# method 1
def solution(s):
    letterDictionary = {}
    for letter in s:
        if letter not in letterDictionary:
            letterDictionary[letter] = 1
        else:
            letterDictionary[letter] += 1
    if sum(letterDictionary.values()) == len(letterDictionary):
        return True
    else:
        return False
    
# test
print solution(string1)
print "Expected: True"

print solution(string2)
print "Expected: False"
