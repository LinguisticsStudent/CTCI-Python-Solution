string1 = "abcdefg"
string2 = "abc123"
string3 = "!@#$%^&*()_+<>? "

# Method 1.2.1
def solution(s):
    newS = ""
    for i in range(len(s)):
        newS += s[len(s)- 1 - i]
    return newS
        
# test
print solution(string1)
print "Expected: gfedcba"

print solution(string2)
print "Expected: 321cba"

print solution(string3)
print "Expected:  ?><+_)(*&^%$#@!"
